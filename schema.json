{
  "schema" : [
    {
      "url" : "https://www.troc-velo.com/",
      "process" : [
        {
          "element" : "div",
          "target" : "blocmaincategorie",
          "type" : "id",
          "occurs": "one",
          "as_list": false
        },
        {
          "element": "div",
          "target": "blocmaincategoriecol",
          "type": "id",
          "occurs": "all",
          "as_list": false
        },
        {
          "element": "a",
          "type": "normal",
          "occurs": "all",
          "as_list": false
        },
        {
          "element": "a",
          "target": "href",
          "type": "attribute",
          "as_list": true
        }
      ]
    },
    {
      "url": "{parent_url}?page={page}",
      "is_dynamic": true,
      "variables": ["parent_url", "page"],
      "process": [
        {
          "element": "div",
          "type": "id",
          "target": "afficheliste-content",
          "occurs": "one"
        }
      ]
    }
  ]
}